{
  "name": "nuxt-compose-icons",
  "version": "0.1.0",
  "private": false,
  "description": "Compose your own Icon components library",
  "repository": {
    "type": "git",
    "url": "https://github.com/use-compose/nuxt-compose-icons"
  },
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./utils": {
      "types": "./dist/runtime/utils/index.d.ts",
      "import": "./dist/runtime/utils/index.js"
    }
  },
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "PORT=3008 nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:generate": "nuxi generate playground",
    "release": "pnpm publish && git push --follow-tags",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "changeset publish",
    "changeset:snapshot": "changeset version --snapshot && changeset publish --tag snapshot",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "4.0.3",
    "@use-compose/logger": "latest",
    "node-html-parser": "7.0.1",
    "svg-parser": "2.0.4",
    "svgo": "4.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "3.1.1",
    "@nuxt/eslint-config": "1.12.1",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.0.3",
    "@nuxt/test-utils": "3.21.0",
    "@types/node": "latest",
    "@types/svg-parser": "2.0.6",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "changelogen": "0.6.2",
    "eslint": "9.34.0",
    "nuxt": "4.0.3",
    "playwright-core": "1.57.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.10"
  }
}
